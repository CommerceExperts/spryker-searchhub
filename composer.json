{
    "name": "commerce-experts/spryker-searchhub",
    "type": "library",
    "description": "CXP SearchHub Spryker Plugin",
    "keywords": [
        "searchhub",
        "commerce experts",
        "cxp",
        "spryker search"
    ],
    "homepage": "https://www.commerce-experts.com",
    "authors": [
        {
            "name": "Siegfried Schuele",
            "email": "siegfried.schuele@commerce-experts.com",
            "role": "COO"
        }
    ],
    "license": "proprietary",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0.0|^7.0.0",
        "spryker/kernel": "^3.0.0",
        "spryker/log": "^3.0.0",
        "spryker/search": ">=8.9.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.1"
    },
    "suggest": {
        "spryker/search-elasticsearch": "makes SearchHubSuggestResultFormatterPlugin available"
    },
    "autoload": {
        "psr-4": {
            "SearchHub\\": "src/SearchHub/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SearchHubTest\\": "tests/SearchHubTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": []
    },
    "config": {
        "sort-packages": true
    }
}
